{
    "name": "celbuxendpoints",
    "displayName": "Celbux Endpoints Language",
    "description": "Adds language specific support to Celbux CSV Endpoint definitions",
    "version": "1.1.1",
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "publisher": "Celbux",
    "contributes": {
    "languages": [{
        "id": "celbuxendpoints",
        "aliases": ["Celbux Endpoints", "celbuxendpoints"],
        "extensions": [".csv"],
        "configuration": "./language-configuration.json"
    }],
	    "grammars": [
	        {
	            "language": "celbuxendpoints",
	            "scopeName": "source.csv",
	            "path": "./syntaxes/celbuxendpoints.tmLanguage.json"
	        }
	    ]
    },
    "icon": "images/logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/Celbux/endpoints-language.git"
    }
}